{
  "manifest_version": 3,
  "name": "Raddish",
  "version": "1.0",
  "description": "Predicts rating changes for Codeforces contests in real-time.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://codeforces.com/*",
    "http://localhost:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/contest/*/standings*",
        "https://codeforces.com/contest/*/room/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}